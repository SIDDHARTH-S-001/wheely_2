<launch>

<rosparam file="$(find wheely_2_description)/launch/controller.yaml" command="load"/>
<node args="lidar_rot_position_controller right_drive_position_controller left_drive_position_controller joint_state_controller " name="controller_spawner" ns="wheely_2" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/wheely_2/joint_states"/>
</node>

</launch>